class Main {
    public static void main(String[] args) {
        for(int i=1;i<=5;i++)
        {
            for(int j=0;j<i;j++)
            {
                 System.out.print("*");
            }
                System.out.println();
        }
        for(int a=4;a>=1;a--) 
        {
            for(int b=1;b<=a;b++)
            {
                  System.out.print("*");
            }
                System.out.println();
       }
    }
}
